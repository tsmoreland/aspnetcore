{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=SunDoeCoffee.db;Pooling=True"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error", 
      "Override": {
          "Microsoft.AspNetCore.Hosting.Diagnostics":  "Information",
          "SunDoeCoffee": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  }
}
